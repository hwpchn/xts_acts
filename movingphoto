async function testMultiExposure() {
  console.info('testMultiExposure Demo');
  try {
    // 获取所有照片资产
    let predicates = new dataSharePredicates.DataSharePredicates();
    let fetchOptions = {
      fetchColumns: [],
      predicates: predicates
    };
    let fetchResult = await phAccessHelper.getAssets fetchOptions);
    let assets = await fetchResult.getObjects();

    // 假设第一个资产是需要修改的对象
    let assetToModify = assets[0];

    // 假设第二个资产是图像，第三个是视频
    let selectedImageAsset = assets[1];
    let selectedVideoAsset = assets[2];

    // 获取 URI
    let imageFileUri = selectedImageAsset uri;
    let videoFileUri = selectedVideoAsset uri;

    // 创建更改请求
    let assetChangeRequest = new photoAccessHelper.MediaAssetChangeRequest assetToModify);

    // 设置效果模式为 MULTI_EXPOSURE
    assetChangeRequest.setEffectMode(photoAccessHelper.MovingPhotoEffectMode.MULTI_EXPOSURE);

    // 添加资源
    assetChangeRequest.addResource(photoAccessHelper.ResourceType.IMAGE, imageFileUri);
    assetChangeRequest.addResource(photoAccessHelper.ResourceType.VIDEO, videoFileUri);

    // 应用更改
    await phAccessHelper.applyChanges assetChangeRequest);
    console.info('apply setEffectMode successfully');
  } catch (err) {
    console.error(`apply setEffectMode failed with error: ${err.code}, ${err.message}`);
  }
}
